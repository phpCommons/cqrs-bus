{
  "name": "php-commons/cqrs-bus",
  "description": "CQRS Bus is Symfony 4.3+ Bundle with Command Bus & QueryBus using Symfony Messanger",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Micha≈Ç Ligus",
      "email": "maveilthain@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "PhpCommons\\CQRSBus\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "symfony/flex": "^1.4",
    "symfony/messenger": "^4.3",
    "symfony/http-kernel": "^4.3",
    "symfony/dependency-injection": "^4.3",
    "symfony/yaml": "^4.3",
    "symfony/config": "^4.3",
    "symfony/framework-bundle": "^4.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
